@model AdminIndexViewModel

@Html.ActionLink("Create New", "Create")

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AdminViewModels.FirstOrDefault().UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdminViewModels.FirstOrDefault().FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdminViewModels.FirstOrDefault().LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdminViewModels.FirstOrDefault().IsEnabled)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.AdminViewModels)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsEnabled)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id })
            </td>
        </tr>
    }

</table>
